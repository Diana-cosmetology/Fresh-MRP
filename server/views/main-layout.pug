doctype html
head
  title Hello, world!

  // Required meta tags
  block headMeta
      meta(charset='utf-8')
      meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport')
      meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')

  // Fonts and icons
  block headResources
      link(href='https://fonts.googleapis.com/css?family=Montserrat:400,700,200' rel='stylesheet')
      link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css')
  // Material Kit CSS
  block headCSS
      link(href='../assets/css/bootstrap.min.css' rel='stylesheet')
      link(href='../assets/css/now-ui-dashboard.css?v=1.1.0' rel='stylesheet')
body
    .wrapper
      .sidebar(data-color='orange')
        //
          Tip 1: You can change the color of the sidebar using: data-color="purple | blue | green | orange | red"
          Tip 2: you can also add an image using data-image tag
        .logo
          a.simple-text.logo-mini(href='http://www.creative-tim.com')
            | CT
          a.simple-text.logo-normal(href='http://www.creative-tim.com')
            | Creative Tim
          .navbar-minimize
            button#minimizeSidebar.btn.btn-simple.btn-icon.btn-neutral.btn-round
              i.now-ui-icons.text_align-center.visible-on-sidebar-regular
              i.now-ui-icons.design_bullet-list-67.visible-on-sidebar-mini
        .sidebar-wrapper
          .user
            .photo
              img(src='../assets/img/mike.jpg')
            .info
              a.collapsed(data-toggle='collapse' href='#collapseExample')
                span
                  | James Amos
                  b.caret
              .clearfix
              #collapseExample.collapse
                ul.nav
                  li
                    a(href='#')
                      span.sidebar-mini-icon MP
                      span.sidebar-normal My Profile
                  li
                    a(href='#')
                      span.sidebar-mini-icon EP
                      span.sidebar-normal Edit Profile
                  li
                    a(href='#')
                      span.sidebar-mini-icon S
                      span.sidebar-normal Settings
          ul.nav
            li.active
              a(href='../examples/dashboard.html')
                i.now-ui-icons.design_app
                p Example 1
            li
              a(data-toggle='collapse' href='#pagesExamples')
                i.now-ui-icons.design_image
                p
                  | Example 2
                  b.caret
              #pagesExamples.collapse
                ul.nav
                  li
                    a(href='../examples/pages/pricing.html')
                      span.sidebar-mini-icon C1
                      span.sidebar-normal Collapse 1
                  li
                    a(href='../examples/pages/timeline.html')
                      span.sidebar-mini-icon C2
                      span.sidebar-normal Collapse 2
      .main-panel
        nav.navbar.navbar-expand-lg.navbar-transparent.navbar-absolute
          .container-fluid
            .navbar-wrapper
              .navbar-toggle
                button.navbar-toggler(type='button')
                  span.navbar-toggler-bar.bar1
                  span.navbar-toggler-bar.bar2
                  span.navbar-toggler-bar.bar3
              a.navbar-brand(href='#pablo') Dashboard
            button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navigation' aria-controls='navigation-index' aria-expanded='false' aria-label='Toggle navigation')
              span.navbar-toggler-bar.navbar-kebab
              span.navbar-toggler-bar.navbar-kebab
              span.navbar-toggler-bar.navbar-kebab
            #navigation.collapse.navbar-collapse.justify-content-end
              form
                .input-group.no-border
                  input.form-control(type='text' value='' placeholder='Search...')
                  span.input-group-addon
                    i.now-ui-icons.ui-1_zoom-bold
              ul.navbar-nav
                li.nav-item
                  a.nav-link(href='#pablo')
                    i.now-ui-icons.media-2_sound-wave
                    p
                      span.d-lg-none.d-md-block Stats
                li.nav-item.dropdown
                  a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='http://example.com' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                    i.now-ui-icons.location_world
                    p
                      span.d-lg-none.d-md-block Some Actions
                  .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
                    a.dropdown-item(href='#') Action
                    a.dropdown-item(href='#') Another action
                    a.dropdown-item(href='#') Something else here
                li.nav-item
                  a.nav-link(href='#pablo')
                    i.now-ui-icons.users_single-02
                    p
                      span.d-lg-none.d-md-block Account
        .panel-header.panel-header-sm
        .content
          .card
            .card-header
              h3 Hello, world!
        footer.footer
          .container-fluid
            nav
              ul
                li
                  a(href='https://www.creative-tim.com')
                    | Creative Tim
                li
                  a(href='http://presentation.creative-tim.com')
                    | About Us
                li
                  a(href='http://blog.creative-tim.com')
                    | Blog
                li
                  a(href='https://github.com/creativetimofficial/now-ui-dashboard/blob/master/LICENSE.md')
                    | MIT License
            .copyright
              | &copy;
              script.
                document.write(new Date().getFullYear())
              | , Designed by
              a(href='http://www.invisionapp.com' target='_blank') Invision
              | . Coded by
              a(href='https://www.creative-tim.com' target='_blank') Creative Tim
              | .
    // Core JS Files
    block bodyJSCore
        script(src='../assets/js/core/jquery.min.js' type='text/javascript')
        script(src='../assets/js/core/popper.min.js' type='text/javascript')
        script(src='../assets/js/core/bootstrap.min.js' type='text/javascript')
        script(src='../assets/js/plugins/perfect-scrollbar.jquery.min.js' type='text/javascript')
        script(src='../assets/js/plugins/moment.min.js')

    block bodyJSPlugins
        // Plugin for Sweet Alert
        script(src='../assets/js/plugins/sweetalert2.min.js')
        // Forms Validations Plugin
        script(src='../assets/js/plugins/jquery.validate.min.js')
        // Plugin for the Wizard, full documentation here: https://github.com/VinceG/twitter-bootstrap-wizard
        script(src='../assets/js/plugins/jquery.bootstrap-wizard.js')
        // Plugin for Select, full documentation here: http://silviomoreto.github.io/bootstrap-select
        script(src='../assets/js/plugins/bootstrap-selectpicker.js' type='text/javascript')
        // Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/
        script(src='../assets/js/plugins/bootstrap-switch.js')
        // Plugin for the DateTimePicker, full documentation here: https://eonasdan.github.io/bootstrap-datetimepicker/
        script(src='../assets/js/plugins/bootstrap-datetimepicker.js')
        // DataTables.net Plugin, full documentation here: https://datatables.net/
        script(src='../assets/js/plugins/jquery.dataTables.min.js')
        // Plugin for Tags, full documentation here: https://github.com/bootstrap-tagsinput/bootstrap-tagsinputs
        script(src='../assets/js/plugins/bootstrap-tagsinput.js')
        // Plugin for Fileupload, full documentation here: http://www.jasny.net/bootstrap/javascript/#fileinput
        script(src='../assets/js/plugins/jasny-bootstrap.min.js')
        // Full Calendar Plugin, full documentation here: https://github.com/fullcalendar/fullcalendar
        script(src='../assets/js/plugins/fullcalendar.min.js')
        // Vector Map plugin, full documentation here: http://jvectormap.com/documentation/
        script(src='../assets/js/plugins/jquery-jvectormap.js')
        // Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/
        script(src='../assets/js/plugins/nouislider.min.js' type='text/javascript')
        // Google Maps Plugin
        script(type='text/javascript' src='https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE')
        // Chart JS
        script(src='../assets/js/plugins/chartjs.min.js')
        // Notifications Plugin
        script(src='../assets/js/plugins/bootstrap-notify.js')
        // Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc
        script(src='../assets/js/now-ui-dashboard.js?v=1.1.0' type='text/javascript')
